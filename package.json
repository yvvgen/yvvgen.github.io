{
  "name": "resume",
  "version": "1.0.0",
  "description": "This is a fairly minimal template for a Yew app that's built with [Trunk].",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "css:build": "tailwindcss -i ./input.css -o ./dist/output.css",
    "css:watch": "tailwindcss -i ./input.css -o ./dist/output.css --watch",
    "dev": "npm run css:watch & trunk serve",
    "css:build:test": "tailwindcss -i ./test.css -o ./dist/test_output.css -c ./tailwind.config.test.cjs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "daisyui": "^5.3.8",
    "tailwindcss": "^4.1.16",
    "tw-animate-css": "^1.4.0"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.16"
  }
}
